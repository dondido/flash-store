{
    "controls": [
        {
            "type": "joystick",
            "dataset": {
                "lock": "y",
                "mode": "fixed",
                "threshold": 0.3
            },
            "mappings": {
                "e": "ArrowRight",
                "w": "ArrowLeft"
            }
        },
        {
            "type": "button",
            "label": "ES",
            "mappings": [{ "code": "Escape" }]
        },
        {
            "type": "button",
            "label": "SP",
            "mappings": [{ "key": "(blank space)", "code": "Space" }]
        }
    
    ]
}