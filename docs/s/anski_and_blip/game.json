{
    "controls": [
        {
            "type": "joystick",
            "dataset": {
                "mode": "fixed"
            },
            "mappings": {
                "e": "ArrowRight",
                "s": "ArrowDown",
                "w": "ArrowLeft"
            }
        },
        {
            "type": "button",
            "label": "U",
            "mappings": [{ "code": "ArrowUp" }]
        },
        {
            "type": "button",
            "label": "SH",
            "mappings": [{ "key": "Shift", "code": "ShiftLeft" }]
        },
        {
            "type": "button",
            "label": "SP",
            "mappings": [{ "key": "(blank space)", "code": "Space" }]
        }
    ]
}