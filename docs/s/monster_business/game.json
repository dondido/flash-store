{
    "controls": [
        {
            "type": "joystick",
            "dataset": {
                "lock": "y",
                "mode": "fixed",
                "threshold": 0.3
            },
            "mappings": {
                "e": "ArrowRight",
                "w": "ArrowLeft"
            }
        },
        {
            "type": "button",
            "label": "U",
            "mappings": [{ "code": "ArrowUp" }]
        },
        {
            "type": "button",
            "label": "SP",
            "mappings": [{ "key": "(blank space)", "code": "Space" }, { "key": "Enter", "code": "Enter", "keyCode": 13, "which": 13 }]
        }
    
    ]
}