{
    "controls": [
        {
            "type": "joystick",
            "dataset": {
                "mode": "fixed",
                "threshold": 0.3
            },
            "mappings": {
                "n": "ArrowUp",
                "e": "ArrowRight",
                "s": "ArrowDown",
                "w": "ArrowLeft"
            }
        },
        {
            "type": "button",
            "label": "EN",
            "mappings": [{ "key": "Enter", "code": "Enter", "keyCode": 13, "which": 13 }]
        },
        {
            "type": "button",
            "label": "SP",
            "mappings": [{ "key": "(blank space)", "code": "Space" }]
        }
    
    ]
}