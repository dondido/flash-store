{
    "url": "avp.swf",
    "title": "Aliens VS Predator",
    "aspectRatio": "550/330",
    "controls": [
        {
            "type": "joystick",
            "dataset": {
                "mode": "fixed"
            },
            "mappings": {
                "n": "ArrowUp",
                "e": "ArrowRight",
                "s": "ArrowDown",
                "w": "ArrowLeft"
            }
        },
        {
            "type": "button",
            "label": "A",
            "mappings": [{ "key": "a" }]
        },
        {
            "type": "button",
            "label": "S",
            "mappings": [{ "key": "s" }]
        },
        {
            "type": "button",
            "label": "D",
            "mappings": [{ "key": "d" }]
        }
    
    ]
}